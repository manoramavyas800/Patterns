5
A
B A
C B A
D C B A
E D C B A


import java.util.Scanner;
public class Test {


    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        for (int i = 1; i <= t; i++) {
            for (int j = i; j >= 1; j--) {
                System.out.print((char) ('A' + j - 1));
                if (j > 1) {
                    System.out.print(" ");
                }
            }
            System.out.println();
        }

 }
}
